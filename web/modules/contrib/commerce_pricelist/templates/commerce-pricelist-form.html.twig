{#
/**
 * @file
 * Template for the price list add/edit form.
 *
 * Available variables:
 * - form: The form.
 *
 * @ingroup themeable
 */
#}
<div class="layout-pricelist-form clearfix">
  <div class="layout-region layout-region-pricelist-main">
    {{ form|without('advanced', 'actions') }}
  </div>
  <div class="layout-region layout-region-pricelist-secondary">
    {{ form.advanced }}
  </div>
  <div class="layout-region layout-region-pricelist-footer">
    {{ form.actions }}
  </div>
</div>
